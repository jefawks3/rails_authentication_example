<h1>Login</h1>
<%= render partial: 'flash' %>
<%= form_with id: dom_id(User, :login), model: User, url: sessions_path, method: :post do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email, required: true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password, required: true %>
  </div>
  <div>
    <%= f.submit 'Login' %>
  </div>
<% end %>

